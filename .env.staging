APP_NAME=Laravel Staging
APP_ENV=staging
APP_KEY=base64:your-generated-staging-key-here
APP_DEBUG=true
APP_URL=https://groupifyglobal.com

LOG_CHANNEL=stack
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

DB_CONNECTION=mysql
DB_HOST=mysql-staging
DB_PORT=3306
DB_DATABASE=laravel_staging
DB_USERNAME=laravel
DB_PASSWORD=your-secure-staging-password

BROADCAST_DRIVER=log
CACHE_DRIVER=redis
FILESYSTEM_DISK=local
QUEUE_CONNECTION=redis
SESSION_DRIVER=redis
SESSION_LIFETIME=120

REDIS_HOST=redis-staging
REDIS_PASSWORD=null
REDIS_PORT=6379

MAIL_MAILER=log
